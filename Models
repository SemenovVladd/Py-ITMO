@property
def e-mail(self)
    return self.__e-mail
@e-mail.setter
def e-mail(self,name:str):
   if tupe (e-mail) is str and len(e-mail)>=5:
       self.__e-mail==e-mail
   else:
      rise ValueError('Ошибка при задании почты пользователя')
@property
def phone(self)
    return self.__phone
